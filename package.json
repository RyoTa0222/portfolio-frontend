    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@next/bundle-analyzer": "^11.1.0",
    "@sentry/react": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "@types/react-transition-group": "^4.4.2",
    "axios": "^0.21.1",
    "babel-plugin-styled-components": "^1.13.2",
    "dayjs": "^1.10.6",
    "framer-motion": "^4",
    "just-throttle": "^2.3.1",
    "next": "11.0.1",
    "next-videos": "1.4.0",
    "npm-run-all": "^4.1.5",
