  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "storybook": "start-storybook -s ./public -p 6006",
    "build:storybook": "build-storybook -o build/storybook",
    "chromatic": "npx chromatic --project-token=PROJECT_TOKEN --exit-once-uploaded",
