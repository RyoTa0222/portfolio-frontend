    "@types/react": "^17.0.14",
    "@types/react-lottie": "^1.2.6",
    "chromatic": "^5.9.2",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "next-compose-plugins": "^2.2.1",
    "node-sass": "^6.0.1",
    "prettier": "2.3.2",
    "storybook-addon-designs": "^6.0.1",
    "storybook-addon-performance": "^0.16.0",
